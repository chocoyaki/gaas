include @PREFIX@/include/Makefile.inc


all: client server

client.o:	client.c
	$(CC) -c $< $(CCFLAGS) -o $@

client:		client.o @PREFIX@/lib/libDIET_client.a
	$(CC) $< $(CCFLAGS) $(CLIENT_LIB) $(LIBS) -o $@

server.o:	server.c
	$(CC) -c $< $(CCFLAGS) -o $@

server:		server.o @PREFIX@/lib/libDIET_SeD.a
	$(CC) $< $(CCFLAGS) $(SED_LIB) $(LIBS) -o $@

clean:
	rm -f *.o client server